    "@types/lodash-es": "^4.17.6",
    "@types/three": "^0.146.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
